{
    "name": "chat-genius",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "db:typegen": "bunx supabase gen types typescript --linked > src/lib/supabase/types.ts",
        "db:dev:reset": "yes | bunx supabase db reset",
        "db:dev:migrate": "bunx supabase db migrate && bun run db:typegen",
        "db:dev:seed": "bun --env-file .env.local scripts/db/seed/dev.ts",
        "db:dev:init": "bun run db:dev:reset && bun run db:typegen && bun run db:dev:seed && bun run ai:generate-embeddings:dev",
        "db:import:slack": "bun --env-file .env.local scripts/db/import/slack.ts",
        "db:import:all": "bun run db:import:slack",
        "db:prod:reset": "yes | bunx supabase db reset --linked",
        "db:prod:push": "bunx supabase db push",
        "db:prod:seed": "bun --env-file .env.production scripts/db/seed/prod.ts",
        "db:prod:deploy": "bun run db:prod:push && bun run db:prod:seed",
        "ai:generate-embeddings:dev": "bun --env-file .env.local scripts/ai/generate-embeddings.ts",
        "ai:generate-embeddings:prod": "bun --env-file .env.production scripts/ai/generate-embeddings.ts",
        "ai:verify-embeddings:dev": "bun --env-file .env.local scripts/ai/verify-embeddings.ts",
        "ai:verify-embeddings:prod": "bun --env-file .env.production scripts/ai/verify-embeddings.ts",
        "ai:search:dev": "bun --env-file .env.local scripts/ai/search-messages.ts",
        "ai:search:prod": "bun --env-file .env.production scripts/ai/search-messages.ts"
    },
    "dependencies": {
        "@radix-ui/react-accordion": "^1.2.2",
        "@radix-ui/react-alert-dialog": "^1.1.4",
        "@radix-ui/react-avatar": "^1.1.2",
        "@radix-ui/react-collapsible": "^1.1.2",
        "@radix-ui/react-dialog": "^1.1.4",
        "@radix-ui/react-dropdown-menu": "^2.1.4",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-popover": "^1.1.4",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-tooltip": "^1.1.6",
        "@react-hook/event": "^1.2.6",
        "@supabase/ssr": "^0.5.2",
        "@supabase/supabase-js": "^2.47.10",
        "@tiptap/extension-placeholder": "^2.11.0",
        "@tiptap/pm": "^2.11.0",
        "@tiptap/react": "^2.11.0",
        "@tiptap/starter-kit": "^2.11.0",
        "@types/lodash": "^4.17.14",
        "@types/uuid": "^10.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "emoji-picker-react": "^4.12.0",
        "immer": "^10.1.1",
        "lucide-react": "^0.469.0",
        "next": "15.1.3",
        "openai": "^4.78.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-error-boundary": "^5.0.0",
        "react-icons": "^5.4.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "use-debounce": "^10.0.4",
        "uuid": "^11.0.4",
        "zod": "^3.24.1",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@shadcn/ui": "^0.0.4",
        "@types/bun": "^1.1.16",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "postcss": "^8",
        "prettier": "3.4.2",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
